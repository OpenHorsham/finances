                    "name": "transaction_reference",
                    "title": "Transaction reference",
                    "description": "Supplier's invoice number or ID",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": false
                    }
                }
            ]
