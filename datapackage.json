    "schemas": {
        "finances": {
            "fields": [
                {
                    "name": "buyer",
                    "title": "Buyer",
                    "description": "Who paid for the transaction; payments made by individuals are considered loans to be reimbursed by the party",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": true
                    }
                },
                {
                    "name": "buyer_uri",
                    "title": "Buyer URI",
                    "description": "URI to uniquely identify the buyer",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#anyURI",
                      "required": false
                    }
                },
                {
                    "name": "supplier",
                    "title": "Supplier",
                    "description": "The supplier of the product or service",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": true
                    }
                },
                {
                    "name": "supplier_uri",
                    "title": "Supplier URI",
                    "description": "URI to uniquely identify the supplier",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#anyURI",
                      "required": false
                    }
                },
                {
                    "name": "currency",
                    "title": "Currency",
                    "description": "ISO 4217 currency code",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": true
                    }
                },
                {
                    "name": "total_charged",
                    "title": "Total charged",
                    "description": "In original currency, including taxes",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#float",
                      "required": false
                    }
                },
                {
                    "name": "amount",
                    "title": "Amount",
                    "description": "Transaction amount in GBP, including VAT",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#float",
                      "required": true
                    }
                },
                {
                    "name": "vat",
                    "title": "VAT paid",
                    "description": "Amount of VAT paid",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#float",
                      "required": true
                    }
                },
                {
                    "name": "balance",
                    "title": "Balance",
                    "description": "Balance of account",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#float",
                      "required": true
                    }
                },
                {
                    "name": "date",
                    "title": "Date",
                    "description": "The date the cost was incurred, not when it was paid",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#date",
                      "required": true
                    }
                },
                {
                    "name": "description",
                    "title": "Description",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": false
                    }
                },
                {
                    "name": "unspsc_code",
                    "title": "UNSPSC code",
                    "description": "United Nations Standard Products and Services Code - http://www.unspsc.org/",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": true
                    }
                },
                {
                    "name": "ec_category",
                    "title": "Electoral Commission spending category",
                    "description": "Spending category A-F as defined in http://www.electoralcommission.org.uk/__data/assets/pdf_file/0013/107104/form-ce-by-election-ca.pdf",
                    "constraints" : {
                      "type": "http://www.w3.org/2001/XMLSchema#string",
                      "required": false
                    }
                },
